{
  "version": 2,
  "builds": [
    {
      "src": "/api/(.*)", "dest": "app.js",
      "use": "@vercel/node",
      "config": {
        "installCommand": "npm install",
        "startCommand": "node app.js"
      }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "app.js" }
  ]

}
